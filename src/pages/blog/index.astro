---
import Layout from "@components/layout.astro";

const arry = ["post-1", "post-2", "post-3", "post-4"];
---

<Layout title="ブログページです">
  <div>Blog</div>
  <ul>
    {
      arry.map((post) => (
        <li>
          <a href={`/posts/${post}/`}>{post}</a>
        </li>
      ))
    }
  </ul>
</Layout>
